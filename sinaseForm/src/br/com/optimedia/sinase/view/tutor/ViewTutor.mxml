						var item:ViewReferencia;
						var dataini:Date
						var datafim:Date
						for (var j:uint = 0 ; j<tutor.exp_prof.length; j++) {
							item = new ViewReferencia();
							grupoReferencia.addElement(item);
							item.exp_prof = new Exp_prof();
							item.exp_prof.data_fim =tutor.exp_prof[j].data_fim
							item.exp_prof.data_ini = tutor.exp_prof[j].data_ini;
							item.exp_prof.funcao = tutor.exp_prof[j].funcao
							item.exp_prof.org_nome = tutor.exp_prof[j].org_nome
							item.exp_prof.idexp_prof = tutor.exp_prof[j].idexp_prof
							item.exp_prof.idtutor = tutor.exp_prof[j].idtutor;
							dataini =   MySQLDateFormatter.mysqlToAs(tutor.exp_prof[j].data_ini as String);
							datafim =  MySQLDateFormatter.mysqlToAs(tutor.exp_prof[j].data_fim as String);
							item.df_fim.data = datafim;
							item.df_ini.data =dataini;
						}
