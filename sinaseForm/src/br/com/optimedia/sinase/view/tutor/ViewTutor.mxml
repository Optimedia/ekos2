				remote.getEscolaridade(result_getEscolaridade);
				remote.getAtuacao(result_getAtuacao);
				remote.getAtividade(result_getAtividade);
				remote.getTecEad(result_getTecEad);
				remote.getTecMdl(result_getTecMdl);
				remote.getBandaLarga(result_getBandaLarga);
				remote.getWebCon(result_getWebCon);
				remote.getOffice(result_getOffice);
				
				if (tutor.idtutor>=1) {
					if (tutor.sexo == "M"){
						rb_masculino.selected = true;
					}else if (tutor.sexo == "F"){
						rb_feminino.selected = true;
					}
					if (tutor.data_nasc !=null) {
						var dataNascimento:Date = MySQLDateFormatter.mysqlToAs(tutor.data_nasc);
						df_nascimento.data = dataNascimento;
					}
					if (tutor.atua_sinase == 0){
						atua_nao.selected= true;
						formQMdalidade.enabled = false;
						formAtividade.enabled = false
					}else if (tutor.atua_sinase == 1){
						atua_sim.selected = true;
						formQMdalidade.enabled = true;
						formAtividade.enabled = true
					}
					if (tutor.idmod_sinase == 1){
						madalidade_aberto.selected= true;
					}else if (tutor.idmod_sinase ==2){
						madalidade_fechado.selected = true;
					}else if (tutor.idmod_sinase ==3){
						madalidade_ambos.selected = true;
					}
					if (tutor.art_publ==1){
						art_publ_sim.selected = true;
						txtArt1.text = tutor.art1;
						txtArt2.text = tutor.art2;
						txtArt3.text = tutor.art3;
					}else if (tutor.art_publ==0){
