			protected function onSalvar(event:MouseEvent):void{
				if (validarForm()) {
					//tutor.cpf
					tutor.nome = txtNome.text;
					tutor.logradouro = txtLogradouro.text;
					tutor.numero = txtNumero.text;
					
					if (rb_feminino.selected == true) {
						tutor.sexo = "F";
					}
					if (rb_masculino.selected == true) {
						tutor.sexo = "M";
					}
					tutor.iduf = cb_UF.selectedItem.iduf;
					tutor.regiao = cb_UF.selectedItem.regiao;
					tutor.bairro = txtBarrio.text;
					tutor.cep = txtCep.text;
					tutor.cidade = txtCidade.text;
					tutor.data_nasc = df_nascimento.selectedDate.fullYear +"-"+Number(df_nascimento.selectedDate.month+1) +"-"+ df_nascimento.selectedDate.date;
					tutor.ddd_cel = txtCelDDD.text;
					tutor.tel_cel = txtCel.text
					tutor.dddfixo = txtTelDDD.text;
					tutor.tel_fixo = txtTel.text;
					tutor.email = txtEmail.text;
					tutor.art1 = txtArt1.text;
					tutor.art2 = txtArt2.text;
					tutor.art3 = txtArt3.text;
					if (art_publ.selectedValue !=null) {
						tutor.art_publ = art_publ.selectedValue.toString()
					}
					tutor.atua_sinase = atuaSinase.selectedValue.toString();
					tutor.disp_fds = fimSemana.selectedValue.toString()
					tutor.disp_semana = disp_semanal.selectedValue.toString()
					tutor.cont_ead = cont_ead.selectedValue.toString();
					tutor.tutor_ead = tutor_ead.selectedValue.toString();
					tutor.aluno_ead = aluno_ead.selectedValue.toString();
					tutor.expec_curso = txtExpec_curso.text;
					tutor.idbanda_larga = cb_bandaLarga.selectedItem.idbanda_larga;
					tutor.idescolaridade = cb_escolaridade.selectedItem.idescolaridade;
					if (sinaseModalidade.selectedValue != null ) {
						tutor.idmod_sinase = sinaseModalidade.selectedValue.toString();
					}
					tutor.idoffice = cb_ofiice.selectedItem.idoffice;
					tutor.idpart_webcon = cb_webconferencia.selectedItem.idpart_webcon;
					tutor.idram_atuac = cb_atuacao.selectedItem.idram_atuac;
					tutor.idtec_ead = cb_tecEad.selectedItem.idtec_ead
					tutor.idtec_mdl = cb_Mdl.selectedItem.idtec_mdl;
					
					tutor.atua_sinase = atuaSinase.selectedValue.toString();
					if (atua_sim.selected==true) {
						tutor.idmod_sinase = sinaseModalidade.selectedValue.toString();
						tutor.ativ_sinase = cb_atividade.selectedItem.nome;
					}
						
					tutor.org_prom_ead = txt_orgaoPromotor.text;
					tutor.out_prog = txtOutros.text;
					tutor.pos_headset = headset.selectedValue.toString();
					tutor.pos_webcam = webcan.selectedValue.toString();
					tutor.tec_webcon = txtTecnologiaUtilizada.text;
					tutor.rel_coleta_dados = rel_coleta_dados.selectedValue.toString();
					
					var itemView:ViewReferencia;
					var exp_prof:Exp_prof;
					tutor.exp_prof = new Array();
					
					tutor.tutoria_moodle = expTutorialmoodle.selectedValue.toString();
					if (expTutorialmoodle_sim.selected == true) {
						
						tutor.instituicao_tutoria1 = txtInstituicao1.text;
						tutor.modalidade_tutoria1= mod_tutoria1.selectedValue.toString();
						tutor.natureza_tutoria1= nat_tutoria1.selectedValue.toString();
						tutor.periodo_tutoria1= per_tutoria1.selectedValue.toString();
						tutor.instituicao_tutoria2=txtInstituicao2.text;
						if (mod_tutoria2.selectedValue != null) {
							tutor.modalidade_tutoria2=mod_tutoria2.selectedValue.toString();
						}
						if (nat_tutoria2.selectedValue != null) {
							tutor.natureza_tutoria2=nat_tutoria2.selectedValue.toString();
						}
						if (per_tutoria2.selectedValue != null) {
							tutor.periodo_tutoria2=per_tutoria2.selectedValue.toString();
						}
						tutor.instituicao_tutoria3=txtInstituicao3.text;
						if (mod_tutoria3.selectedValue != null) {
							tutor.modalidade_tutoria3=mod_tutoria3.selectedValue.toString();
						}
						if (nat_tutoria3.selectedValue != null) {
							tutor.natureza_tutoria3=nat_tutoria3.selectedValue.toString()
						}
						if (per_tutoria3.selectedValue != null) {
							tutor.periodo_tutoria3=per_tutoria3.selectedValue.toString();
						}
					}
					if (webCom_tutor_sim.selected == true) {
						if (txtWebCom_tutor.text=="") {
							tutor.tutoria_webconference = "1";
						}else {
							tutor.tutoria_webconference = txtWebCom_tutor.text;
						}
					}else {
						tutor.tutoria_webconference = "0"; 
					}
					tutor.lattes = txtLattes.text;
					tutor.participou_sinase = participouSinase.selectedValue.toString();
					for (var i:uint=0;i<grupoReferencia.numChildren; i++) {
						itemView = grupoReferencia.getChildAt(i) as ViewReferencia;
						exp_prof = new Exp_prof(); 
						exp_prof.funcao = itemView.txtFuncao.text;
						exp_prof.idexp_prof = itemView.exp_prof.idexp_prof;
						exp_prof.idtutor = itemView.exp_prof.idtutor;
						exp_prof.org_nome = itemView.txtOrgaoNome.text;
						exp_prof.data_ini = itemView.df_ini.selectedDate.fullYear+"-"+ Number(itemView.df_ini.selectedDate.month+1)+ "-" + itemView.df_ini.selectedDate.date;
						exp_prof.data_fim =itemView.df_fim.selectedDate.fullYear+"-"+ Number(itemView.df_fim.selectedDate.month+1) + "-" + itemView.df_fim.selectedDate.date;
						
						tutor.exp_prof.push(exp_prof);
					}
					remote.saveTutor(tutor, salvarTutor_result)
				}else {
					Alert.show("Verifique os campos em vermelho.");
				}
				
			}
			private function salvarTutor_result(event:ResultEvent):void {
				if (event.result==true) {
					Alert.show("Usuário Cadastrado com sucesso.");
					var tutorEvent:TutorEvent = new TutorEvent(TutorEvent.CADASTRO_SUCESSO);
					dispatchEvent(tutorEvent);
				}else{
					Alert.show("Erro no cadastro, tente novamente.");
				}
			}
			
			protected function onWebCon_tutor(event:MouseEvent):void{
				if (webCom_tutor_sim.selected==true) {
					txtWebCom_tutor.editable = true;
				}else {
					txtWebCom_tutor.editable = false;
					txtWebCom_tutor.text = "";
				}
			}
			
			protected function visible_experienciaMoodle(event:MouseEvent):void	{
				if (expTutorialmoodle_sim.selected == true){
					mostarItemMoodle()
				}else {
					ocultaItemMoodle();
				}
				
				
			}
			
		]]>
