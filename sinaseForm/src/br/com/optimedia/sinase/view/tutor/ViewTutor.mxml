			protected function decladoLido(event:MouseEvent):void{
				if (cb_validos.selected == true) {
					btn_enviar.enabled = true;
				}else {
					btn_enviar.enabled = false;
				}
				
			}
			
			protected function modalidadeSelecao(event:MouseEvent):void{
				if (atua_sim.selected==true) {
					formQMdalidade.enabled=true;
					formAtividade.enabled=true;
				}else{
					formQMdalidade.enabled=false;
					formAtividade.enabled=false;
				}
			}
			
			protected function adicionarReferencia(item:Array = null):void{
				var referencia:ViewReferencia = new ViewReferencia();
				grupoReferencia.addElement(referencia);
				if (item) {
					referencia.txtFuncao = item['funcao'];
					referencia.txtOrgaoNome = item['org_nome'];
					referencia.df_ini.data = MySQLDateFormatter.mysqlToAs(item['data_ini'] as String);
					referencia.df_fim.data = MySQLDateFormatter.mysqlToAs(item['data_fim'] as String);
				}
				
			}
