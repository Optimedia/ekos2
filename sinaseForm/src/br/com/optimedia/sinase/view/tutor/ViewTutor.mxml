					nat_tutoria2.selectedValue =tutor.natureza_tutoria2;
					per_tutoria2.selectedValue =tutor.periodo_tutoria2;
					
					mod_tutoria3.selectedValue =tutor.modalidade_tutoria3;
					nat_tutoria3.selectedValue =tutor.natureza_tutoria3;
					per_tutoria3.selectedValue =tutor.periodo_tutoria3;
					
				}else {
					expTutorialmoodle_nao.selected = true;
