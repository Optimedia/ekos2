			protected function cb_areaAtuacao_clickHandler(event:DropDownEvent):void{
				var obj:Object = new Object();
				//event.currentTarget.selectedItem
				if (event.currentTarget == cb_areaAtuacao && cb_areaAtuacao.selectedItem) {
					obj = cb_areaAtuacao.selectedItem;
					subAreaAtuacao.removeAllElements();
				}else {
					var select:DropDownList = new DropDownList();
					select = event.currentTarget as DropDownList;
					obj = select.selectedItem
				}
				var subList:ArrayCollection = new ArrayCollection();
				var item:AreaAtuacaoVO = new AreaAtuacaoVO();
				if (obj.subitem) {
					for each (var ob:Object in obj.subitem) {
						if (event.currentTarget != cb_areaAtuacao) {
							DropDownList(event.currentTarget).enabled = false;
						}
						item = new AreaAtuacaoVO();
						item.id = ob.id;
						item.id_pai= ob.id_pai;
						item.area= ob.area;
						item.subitem= ob.subitem;
						subList.addItem(item);
					} 
					var cb:DropDownList = new DropDownList();
					cb.dataProvider = subList;
					cb.width =400;
					cb.labelField = "area";
					subAreaAtuacao.addElement(cb);
					cb.addEventListener(DropDownEvent.CLOSE,cb_areaAtuacao_clickHandler);
				}else {
					id_area_atuacaoSel = DropDownList(event.currentTarget).selectedItem.id;
