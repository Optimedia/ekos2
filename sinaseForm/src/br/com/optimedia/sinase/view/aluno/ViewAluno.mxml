			private function result_getPerguntaArray(event:ResultEvent):void{
				var perguntaVO:PerguntaVO = new PerguntaVO()
				perguntaVO = event.result as PerguntaVO;
				var pergunta:Label = new Label();
				pergunta.y = posY;
				posY +=27;
				pergunta.text = idpergunta + " - " + perguntaVO.descricao;
				var alternativa:CheckBox;
				var respostaVO:RespostaVO;
				grupoPergunta.addElement(pergunta);
				var qual:TextInput;
				var hgrupo:HGroup;
				var respostaSel:RespostaVO;
				if ( perguntaVO.resposta) {
					for (var i:uint = 0 ; i < perguntaVO.resposta.length; i++) {
						respostaVO = new RespostaVO();
						respostaVO = perguntaVO.resposta[i] as RespostaVO;
						alternativa = new CheckBox();
						alternativa.x = posX;
						alternativa.label = respostaVO.value_resposta;
						alternativa.name = "item_" + respostaVO.idresposta;
						alternativa.id = "item_" + respostaVO.idresposta;
						
						
						alternativa.y = posY;
						grupoPergunta.addElement(alternativa);
						respostaSel = new RespostaVO();
						respostaSel = marcaChebox(respostaVO.idresposta);
						if (respostaSel.idresposta==respostaVO.idresposta){
							alternativa.selected = true;
						}
						if (respostaVO.value_resposta.toLowerCase()== "outro. qual?"){
							alternativa.addEventListener(MouseEvent.CLICK,selecionaQual);
							posY +=27;
							qual = new TextInput();
							qual.editable=false;
							qual.enabled=false;
							qual.id = "qual_"+ respostaVO.idresposta;
							qual.name = "qual_"+ respostaVO.idresposta;
							qual.percentWidth = 100;
							qual.x = posX;
							qual.y = posY;
							qual.text = respostaSel.value_resposta;
							grupoPergunta.addElement(qual);
						}
						posY +=27;
						}
					}
					posY +=27;
					idpergunta++;
					if ((idpergunta !=3) && (idpergunta<=5)) {
						remote.getPerguntaArray(idpergunta, result_getPerguntaArray);
					}else if (idpergunta==3){
						pergunta = new Label();
						pergunta.text = "3 - Você considera fundamental a realização de aulas presenciais mesmo em cursos EAD? Justifique sua resposta:"
						pergunta.y = posY;
						posY +=27;
						grupoPergunta.addElement(pergunta);
	
						hgrupo = new HGroup()
						hgrupo.y = posY;
						hgrupo.x = posX;
						hgrupo.percentWidth = 100;
						grupoPergunta.addElement(hgrupo);
						
						radio_sim = new RadioButton()
						radio_sim.groupName = "pergunta3";
						radio_sim.label = "Sim";
						hgrupo.addElement(radio_sim);
						if (aluno.presenca_ead==1) {
							radio_sim.selected = true
						}
						
						radio_nao = new RadioButton()
						radio_nao.groupName = "pergunta3";
						radio_nao.label = "Não";
						if (aluno.presenca_ead==0) {
							radio_nao.selected = true
						}
						hgrupo.addElement(radio_nao);
						posY +=27;
						
						txtAlternativa3 = new TextArea();
						txtAlternativa3.text = aluno.just_presenca;
						txtAlternativa3.percentWidth = 100;
						txtAlternativa3.height = 80;	
						txtAlternativa3.x = posX
						txtAlternativa3.y = posY;
						grupoPergunta.addElement(txtAlternativa3);
						posY +=80;
						posY +=27;
						
						idpergunta++;
						remote.getPerguntaArray(idpergunta, result_getPerguntaArray);
					}
				
			}
			private function selecionaQual(event:MouseEvent):void{
				var nome:String = event.currentTarget.id;
				var sub:String = nome.substr(5,3);
				var obj:*
				var campoQual:TextInput;
				var itemQual:Boolean = false;
				for (var j:uint=0; j<grupoPergunta.numElements; j ++) {
					if (itemQual ==true) {
						if (Object(grupoPergunta.getElementAt(j)).name=="qual_" + sub) {
							TextInput(grupoPergunta.getElementAt(j)).enabled = true
							TextInput(grupoPergunta.getElementAt(j)).editable = true
						}
					}else {
						if (Object(grupoPergunta.getElementAt(j)).name=="qual_" + sub) {
							TextInput(grupoPergunta.getElementAt(j)).enabled = false;
							TextInput(grupoPergunta.getElementAt(j)).editable = false;
						}
					}
					if (Object(grupoPergunta.getElementAt(j)).name==CheckBox(event.currentTarget).name){
						if(CheckBox(event.currentTarget).selected==true) {
							itemQual=true
							//campoQual = grupoPergunta.getChildByName("qual_" + sub) as TextInput;
						}
					}
				}
				
			}
