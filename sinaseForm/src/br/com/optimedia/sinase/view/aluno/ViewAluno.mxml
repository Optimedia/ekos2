					aluno.data_nasc =  df_nascimento.selectedDate.fullYear +"-"+Number(df_nascimento.selectedDate.month+1) +"-"+ df_nascimento.selectedDate.date;
					aluno.ddd_cel = txtCelDDD.text;
					aluno.dddfixo = txtTelDDD.text;
					if (def_sim.selected == true) {
						aluno.deficiente = txtDesfQual.text;
					}
					aluno.disp_fds = fimSemana.selectedValue.toString();
					aluno.disp_sem = disp_semanal.selectedValue.toString();
					aluno.email = txtEmail.text;
					aluno.exp_ead = exp_ead.selectedValue.toString();
					aluno.headset = headset.selectedValue.toString();
					aluno.idatua_org = atua_orgao.selectedValue.toString();
					aluno.idbanda_larga = cb_bandaLarga.selectedItem.idbanda_larga;
					aluno.idturma_sinase = turmaSinase.selectedValue.toString();
					//aluno.idcapacitacao_sinase = cb_cursoSEDH.selectedItem.idcapacitacao_sinase;
					var arrayCursoSDH:Array = new Array();
					for (var j:uint=0; j<grupoSDH.numElements; j ++) {
						if (CheckBox(grupoSDH.getElementAt(j)).selected ==true){
							arrayCursoSDH.push(CheckBox(grupoSDH.getElementAt(j)).data.idcapacitacao_sinase)
						}
					}
					aluno.idcapacitacao_sinase = arrayCursoSDH;
					aluno.idcarga_hora = horas.selectedValue.toString();
					aluno.idescolaridade = cb_escolaridade.selectedItem.idescolaridade;
					aluno.idinternet = usoInternet.selectedValue.toString();
					aluno.idpart_webcon = cb_webconferencia.selectedItem.idpart_webcon;
					aluno.idram_atuac = id_area_atuacaoSel;
					if (tipoAreaQual==true) {
						aluno.area_atuac_qual = txtAtuacaoQual.text;
					}else {
						aluno.area_atuac_qual = "";
					}
					aluno.idtec_ead = cb_tecEad.selectedItem.idtec_ead;
					aluno.iduf = cb_UF.selectedItem.iduf;
					//aluno.ind_moodle =
					if (radio_sim.selected==true) {
						aluno.presenca_ead = 1;
						aluno.just_presenca = txtAlternativa3.text;
					}else if (radio_nao.selected==true) {
						aluno.presenca_ead = 0;
						aluno.just_presenca = txtAlternativa3.text;
					}
					aluno.logradouro = txtLogradouro.text;
					aluno.material_esp = material.selectedValue.toString();
					//aluno.naturalidade 
					aluno.nome = txtNome.text;
					aluno.nome_mae = txtMae.text;
					aluno.nome_pai = txtPai.text;
					//aluno.orgao_expeditor
					//aluno.pontuacao
					//aluno.presenca_ead
					//aluno.registro
					aluno.sexo=Sexo.selectedValue.toString();
					aluno.tel_cel = txtCel.text;
					aluno.tel_fixo = txtTel.text;
					aluno.webcam =  webcan.selectedValue.toString();
					var nome:String;
					var nome2:uint
					var nomeItem:String;
					var cb:CheckBox;
					var campoQual:TextInput
					aluno.respostas = new Array();
					
					for (var j:uint=0; j<grupoPergunta.numElements; j ++) {
						nome = new String();
						nome = Object(grupoPergunta.getElementAt(j)).name;
						nomeItem = nome.substr(0,4);
						if (nomeItem=="item") {
							cb = new CheckBox();
							cb = CheckBox(grupoPergunta.getElementAt(j));
							if (cb.selected==true){
								nome2 = uint(nome.substr(5,9));
								respota = new RespostaVO();
								respota.idresposta = nome2;
								if (cb.label.toLowerCase() =="Outro. qual?") {
									campoQual = new TextInput();
									campoQual = grupoPergunta.getChildByName("qual_" + nome2) as TextInput;
									respota.value_resposta = TextInput(grupoPergunta.getChildByName("qual_" + nome2)).text;
								}
								aluno.respostas.push(respota);
							}
						}
					}
					btn_enviar.enabled = false;
					remote.saveAluno(aluno,result_saveAluno);
				}else {
					Alert.show("Verifique os campos em vermelho!", "Erro");

				}
			}
			private function result_saveAluno(event:ResultEvent):void{
				if (event.result== true) {
					var alunoEvent:AlunoEvent = new AlunoEvent(AlunoEvent.CADASTRO_SUCESSO);
					dispatchEvent(alunoEvent);
				}else{
					Alert.show("Erro ao salvar as informações, tente novamente!","Erro");
				}
				btn_enviar.enabled = true;
			}
			protected function onValidar(event:MouseEvent):void{
				if (idValido.selected == true){
					btn_enviar.enabled = true;
				}else {
					btn_enviar.enabled= false;
				}
			}
		]]>
