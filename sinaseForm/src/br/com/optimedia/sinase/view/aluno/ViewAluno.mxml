			private function result_getAreaAtuacao(event:ResultEvent):void{
				var proxy:ObjectProxy = ObjectProxy(event.result);
			//	var obj:Object = proxy.object_proxy::object;
				
				listaAtuacao = new ArrayCollection()
				var item:AreaAtuacaoVO = new AreaAtuacaoVO();
				var item2:AreaAtuacaoVO = new AreaAtuacaoVO();
				var item3:AreaAtuacaoVO = new AreaAtuacaoVO();
				
				var itemsSelecionado:Array = new Array();
				var listaSelecionado:Array = new Array();
				for each (var ob:Object in proxy) {
					 item = new AreaAtuacaoVO();
					 item.id = ob.id;
					 item.id_pai= ob.id_pai;
					 item.area= ob.area;
					 item.subitem= ob.subitem;
					 listaAtuacao.addItem(item);
					 
					 if ((aluno.idram_atuac!=0)) {
						for each (var obs2:Object in ob.subitem) {
							 item2 = new AreaAtuacaoVO();
							 item2.id = obs2.id;
							 item2.id_pai= obs2.id_pai;
							 item2.area= obs2.area;
							 item2.subitem= obs2.subitem;
							 if (aluno.idram_atuac == item2.id){
								 
								//cb_areaAtuacao.selectedItem = item;
								itemsSelecionado.push(item.id);
								itemsSelecionado.push(item2.id);
								
								listaSelecionado.push(item);
								listaSelecionado.push(item2);
								 
							 }else if (Array(item2.subitem).length<0){
								 for each (var obs3:Object in obs2.subitem) {
									 item3 = new AreaAtuacaoVO();
									 item3.id = obs3.id;
									 item3.id_pai= obs3.id_pai;
									 item3.area= obs3.area;
									 item3.subitem= obs3.subitem;
									 if (aluno.idram_atuac == item3.id){
										// cb_areaAtuacao.selectedItem = item;
										 
										 itemsSelecionado.push(item.id);
										 itemsSelecionado.push(item2.id);
										 itemsSelecionado.push(item3.id);
										 
										 listaSelecionado.push(item);
										 listaSelecionado.push(item2);
										 listaSelecionado.push(item3);
									 }
								 }
							 }
						 }
					 }
				 }
				if (aluno.area_atuac_qual!=""){
					tipoAreaQual = true
					AreaAtuacaoQual.visible = true;
					AreaAtuacaoQual.includeInLayout = true;
					txtAtuacaoQual.text = aluno.area_atuac_qual;
				}
				if ((aluno.idram_atuac!=0)) {
						marcarRetornoAreaAtuacao(itemsSelecionado, listaSelecionado);
				}
			}
			private function marcarRetornoAreaAtuacao(itemsSelecionado:Array , lista:Array):void {
				
				var cb:DropDownList=new DropDownList();
				var subList:ArrayCollection = new ArrayCollection();
				var item:AreaAtuacaoVO;
				var s:uint=0;
				var select:uint;
				
				for (var i:uint = 0; i<lista.length; i++) {
					if (i==0) {
						cb_areaAtuacao.selectedItem = lista[i];
						
						subList = new ArrayCollection();
						for each (var ob:Object in lista[i].subitem) {
							 if (itemsSelecionado[1]==ob.id){
								select = s;
							}
							s++; 
							item = new AreaAtuacaoVO();
							item.id = ob.id;
							item.id_pai= ob.id_pai;
							item.area= ob.area;
							item.subitem= ob.subitem;
							subList.addItem(item);
						} 
						cb = new DropDownList();
						cb.dataProvider = subList;
						cb.width =400;
						cb.labelField = "area";
						subAreaAtuacao.addElement(cb);
						cb.addEventListener(DropDownEvent.CLOSE,cb_areaAtuacao_clickHandler);
						cb.selectedIndex = select;
						if (lista.length>2){
							cb.enabled = false;
						}
					}else {
						var t:uint = Number(i)+Number(1);
						subList = new ArrayCollection();
						for each (var ob:Object in lista[i].subitem) {
							if (itemsSelecionado[1]==ob.id){
								select = s;
							}
							s++; 
							item = new AreaAtuacaoVO();
							item.id = ob.id;
							item.id_pai= ob.id_pai;
							item.area= ob.area;
							item.subitem= ob.subitem;
							subList.addItem(item);
						} 
						if (subList.length>0) {
							cb = new DropDownList();
							cb.dataProvider = subList;
							cb.width =400;
							cb.labelField = "area";
							subAreaAtuacao.addElement(cb);
							cb.addEventListener(DropDownEvent.CLOSE,cb_areaAtuacao_clickHandler);
							cb.selectedIndex = select;
							if (lista.length>2){
								cb.enabled = false;
							}
						}
					}
				}
			}
